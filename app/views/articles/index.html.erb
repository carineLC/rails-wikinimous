

<div class="container">
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 article">
      <h1>My articles</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2 article">
      <% @articles.each do |article| %>
        <div>
          <h2><%= link_to article.title, article_path(article) %></h2>
          <%= article.content %>
        </div>
      <% end %>
    </div>
  </div>
</div>
